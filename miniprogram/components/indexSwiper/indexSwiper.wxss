.index-swiper{
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  overflow: hidden;
}

.swiperImg{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
  transform: scale(0.8);
  transition: 0.4s
}

.active.swiperImg{
  width: 100%;
  height: 100%;
  border-radius: 20rpx;
  transform: scale(1);
  transition: 0.4s;
}